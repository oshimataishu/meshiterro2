<h1>新規投稿</h1>

<%= form_with model:@postimage do |f| %>
  <h4><%= f.label "画像" %></h4>
  <%= f.file_field :image, accept: 'images/*' %>

  <h4><%= f.label "ショップ名" %></h4>
  <%= f.text_field :shop_name %>

  <h4><%= f.label "説明" %></h4>
  <%= f.text_area :caption %>

  <%= f.submit "投稿" %>

<% end %>

